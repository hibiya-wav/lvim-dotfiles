// Place your key bindings in this file to override the defaults
[
  {
    "key": "alt+cmd+r",
    "command": "workbench.action.files.toggleActiveEditorReadonlyInSession",
    "when": "editorTextFocus"
  },
  {
    "key": "escape",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "!editorTextFocus"
  }
]
